<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/cui_select_item_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="sssssssssssssssssssssssssssssssssssssssssss"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/cui_select_item_select"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/cui_select_item_select"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/cui_icon_right_24"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>